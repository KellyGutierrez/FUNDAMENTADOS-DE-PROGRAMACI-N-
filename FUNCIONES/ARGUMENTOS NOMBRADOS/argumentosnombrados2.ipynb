        "def cargar():\n",
        "    lista=[]\n",
        "    for x in range(10):\n",
        "        valor=int(input(\"Ingrese valor:\"))\n",
        "        lista.append(valor)\n",
        "    return lista\n",
        "\n",
        "\n",
        "def imprimir(lista):\n",
        "    for x in range(len(lista)):\n",
        "        print(lista[x], end=\",\")\n",
        "\n",
        "\n",
        "# bloque principal\n",
        "\n",
        "lista=cargar()\n",
        "imprimir(lista)"
